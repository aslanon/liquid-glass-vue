{
	"name": "@aslanonur/liquid-glass-vue",
	"version": "1.1.2",
	"description": "Apple's Liquid Glass effect implementation for Vue 3 + Nuxt 3. A liquid glass component with real-time displacement mapping, chromatic aberration, and glassmorphism effects.",
	"private": false,
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./style.css": "./dist/style.css"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "nuxt build",
		"build:lib": "vite build",
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"prepublishOnly": "npm run build:lib"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"dependencies": {},
	"devDependencies": {
		"@nuxtjs/tailwindcss": "^6.12.2",
		"@nuxt/devtools": "latest",
		"@types/node": "^20.0.0",
		"@vitejs/plugin-vue": "^5.0.0",
		"autoprefixer": "^10.4.20",
		"lucide-vue-next": "^0.468.0",
		"nuxt": "^3.15.0",
		"postcss": "^8.5.1",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"vite": "^6.0.0",
		"vite-plugin-dts": "^3.0.0",
		"vue": "latest",
		"vue-router": "latest",
		"vue-tsc": "^1.8.0"
	},
	"keywords": [
		"vue",
		"vue3",
		"nuxt",
		"nuxt3",
		"component",
		"library",
		"typescript",
		"liquid-glass",
		"apple",
		"glass-effect",
		"glassmorphism",
		"displacement-mapping",
		"chromatic-aberration",
		"svg-filters",
		"ui-component"
	],
	"author": "Liquid Glass Vue Team",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aslanon/liquid-glass-vue.git"
	},
	"homepage": "https://github.com/aslanon/liquid-glass-vue#readme",
	"bugs": {
		"url": "https://github.com/aslanon/liquid-glass-vue/issues"
	}
}
